web:    java $JAVA_OPTS -Dserver.port=$PORT -jar target/*.jar --okta.client.orgUrl=$OKTA_CLIENT_ORG_URL --okta.client.token=$OKTA_CLIENT_TOKEN --security.oauth2.client.clientId=$SECURITY_OAUTH2_CLIENT_CLIENT_ID --security.oauth2.client.clientSecret=$SECURITY_OAUTH2_CLIENT_CLIENT_SECRET --security.oauth2.client.accessTokenUri=$SECURITY_OAUTH2_CLIENT_ACCESS_TOKEN_URI --security.oauth2.client.userAuthorizationUri=$SECURITY_OAUTH2_CLIENT_USER_AUTHORIZATION_URI --security.oauth2.client.clientAuthenticationScheme=$SECURITY_OAUTH2_CLIENT_CLIENT_AUTHENTICATION_SCHEME --security.oauth2.client.scope=$SECURITY_OAUTH2_CLIENT_SCOPE --security.oauth2.resource.userInfoUri=$SECURITY_OAUTH2_RESOURCE_USER_INFO_URI --security.oauth2.resource.preferTokenInfo=$SECURITY_OAUTH2_RESOURCE_PREFER_TOKEN_INFO --security.oauth2.resource.openIdConfigUri=$SECURITY_OAUTH2_RESOURCE_OPEN_ID_CONFIG_URI
